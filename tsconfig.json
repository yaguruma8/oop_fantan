{
    "compilerOptions": {
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "strict": true,                  
        "esModuleInterop": true, 
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
            "@app/*" : ["./src/app/*"],
            "@view/*" : ["./src/view/*"]
        }
    },
}